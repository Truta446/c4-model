@startuml BibliotecaComponent
!includeurl https://raw.githubusercontent.com/plantuml-stdlib/C4-PlantUML/master/C4.puml

class Book {
  - String title
  - String author
  - Date publishedDate
  - boolean isAvailable

  + getTitle() : String
  + getAuthor() : String
  + getPublishedDate(): Date
  + checkAvailability() : boolean
}

@enduml